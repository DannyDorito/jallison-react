    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.5.2",
    "apollo3-cache-persist": "^0.15.0",
    "eslint-config-prettier": "^9.1.0",
    "graphql": "^16.8.1",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.75.0",
    "svgo": "^3.2.0",
    "usehooks-ts": "^3.1.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@tsconfig/recommended": "^1.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "3.2.5",
    "typescript": "^5.4.3"
  },
  "overrides": {
    "typescript": "^5.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update": "npm install --save-dev @types/react@latest @types/react-dom@latest && npm install react-router-dom && npm install react-scripts",
    "svgs": "npx @svgr/cli --typescript --out-dir src/svgs --ignore-existing src/raw-svgs",
    "outdated": "npm outdated --depth=10",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
